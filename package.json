{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "app.js",
  "config": {
    "public_url": "/",
    "asset_url": "/static/"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev:eleventy": "eleventy --serve --quiet",
    "build:eleventy": "npx @11ty/eleventy --config=.eleventy.js",
    "build:snowpack": "snowpack build",
    "build": "ELEVENTY_ENV=production npm run build:snowpack",
    "start": "snowpack dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "https://erikgelderblom.com/",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@snowpack/plugin-postcss": "^1.0.5",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-sass": "^1.0.1",
    "autoprefixer": "^10.4.19",
    "babel-plugin-lodash": "^3.3.4",
    "critical": "^7.1.2",
    "cssnano": "^7.0.1",
    "fitvids": "^2.1.1",
    "focus-visible": "^5.2.0",
    "image-size": "^1.1.1",
    "inter-ui": "^4.0.2",
    "load-bmfont": "^1.4.1",
    "lodash-es": "^4.17.15",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^4.0.0",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-link-attributes": "^4.0.1",
    "nanobus": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-custom-media": "^10.0.6",
    "postcss-media-minmax": "^5.0.0",
    "postcss-preset-env": "^9.5.13",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.77.1",
    "snowpack": "^3.8.8",
    "three": "^0.164.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 2 versions",
    "> 1%",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ]
}
