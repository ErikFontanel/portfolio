{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .cache dist",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --serve --quiet",
    "dev:parcel": "NODE_ENV=development parcel watch ./src/assets/js/index.js --out-dir ./dist/assets --public-url /assets",
    "build": "run-s clean prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy --output=./build",
    "prod:parcel": "parcel build ./src/assets/img/* ./src/assets/css/app.scss ./src/assets/js/index.js ./src/assets/fonts/* --out-dir ./dist/assets --public-url /assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "https://github.com/ErikFontanel/portfolio-temp#readme",
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.4",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.4",
    "sass": "^1.25.0"
  }
}
