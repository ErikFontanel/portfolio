{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "app.js",
  "config": {
    "public_url": "/",
    "asset_url": "/static/"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev:eleventy": "eleventy --serve --quiet",
    "build:vite": "eleventy --config=.eleventy.js",
    "build": "ELEVENTY_ENV=production npm run build:vite",
    "start": "eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "https://erikgelderblom.com/",
  "dependencies": {
    "@11ty/eleventy": "^3.1.1",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-vite": "^6.0.0",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "autoprefixer": "^10.4.19",
    "babel-plugin-lodash": "^3.3.4",
    "critical": "^7.2.1",
    "cssnano": "^7.0.7",
    "fitvids": "^2.1.1",
    "focus-visible": "^5.2.1",
    "image-size": "^2.0.2",
    "inter-ui": "^4.1.0",
    "load-bmfont": "^1.4.2",
    "lodash-es": "^4.17.15",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^4.0.0",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-link-attributes": "^4.0.1",
    "nanobus": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-custom-media": "^11.0.6",
    "postcss-media-minmax": "^5.0.0",
    "postcss-preset-env": "^10.2.0",
    "rollup-plugin-critical": "^1.0.15",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.89.1",
    "three": "^0.177.0",
    "vite": "^6.3.5",
    "vite-plugin-eleventy": "^0.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 2 versions",
    "> 1%",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ]
}
