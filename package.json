{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "app.js",
  "config": {
    "public_url": "/",
    "asset_url": "/static/"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "fonts": "cp \"./node_modules/inter-ui/Inter\\ \\(web\\)/Inter.var.woff2\" src/fonts/",
    "dev:eleventy": "eleventy --serve --quiet --config=.eleventy.js",
    "build:eleventy": "npx @11ty/eleventy --config=.eleventy.js",
    "build:snowpack": "snowpack build",
    "build": "NODE_ENV=production ELEVENTY_ENV=production npm run build:eleventy && npm run build:snowpack",
    "start": "snowpack dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "https://erikgelderblom.com/",
  "dependencies": {
    "load-bmfont": "^1.4.1",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@snowpack/plugin-optimize": "^0.2.9",
    "@snowpack/plugin-parcel": "^2.0.5",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-sass": "^1.0.1",
    "@swup/overlay-theme": "^1.1.1",
    "autoprefixer": "^10.3.2",
    "babel-plugin-lodash": "^3.3.4",
    "critical": "^4.0.1",
    "cssnano": "^5.0.8",
    "fitvids": "^2.1.1",
    "focus-visible": "^5.2.0",
    "image-size": "^1.0.0",
    "inter-ui": "^3.19.3",
    "markdown-it-anchor": "^8.1.3",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-link-attributes": "^3.0.0",
    "nanobus": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.2.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-media-minmax": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.38.1",
    "snowpack": "^3.8.7",
    "three": "^0.132.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 2 versions",
    "> 1%",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ]
}