{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "app.js",
  "config": {
    "public_url": "/",
    "asset_url": "/static/"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev:eleventy": "eleventy --serve --quiet",
    "dev:parcel": "parcel watch ./src/app.js",
    "dev": "npx run-p \"dev:*\"",
    "build:critical": "./node_modules/.bin/critical dist/index.html -c src/css/app.css --base / > src/css/critical.css",
    "build:eleventy": "npx @11ty/eleventy --config=.eleventy.js",
    "build:parcel": "parcel build ./src/app.js --no-content-hash",
    "build:postcss": "mkdir ./src/css && postcss src/scss/**/*.css -d src/css",
    "build:snowpack": "snowpack build",
    "build:clean": "rm -rf ./dist/static/scss",
    "build": "NODE_ENV=production ELEVENTY_ENV=production run-s build:snowpack build:clean",
    "start": "snowpack dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "https://erikgelderblom.com/",
  "dependencies": {
    "load-bmfont": "^1.4.1",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@snowpack/plugin-parcel": "^2.0.5",
    "@snowpack/plugin-postcss": "^1.0.5",
    "@snowpack/plugin-sass": "^1.0.1",
    "@swup/overlay-theme": "^1.1.1",
    "autoprefixer": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "critical": "^2.0.5",
    "cssnano": "^4.1.10",
    "fitvids": "^2.1.1",
    "focus-visible": "^5.2.0",
    "image-size": "^0.9.3",
    "markdown-it-anchor": "^6.0.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-link-attributes": "^3.0.0",
    "nanobus": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "postcss": "^8.1.2",
    "postcss-cli": "^8.2.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-media-minmax": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.27.0",
    "snowpack": "^2.15.1",
    "three": "^0.122.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 2 versions",
    "> 1%",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ]
}
