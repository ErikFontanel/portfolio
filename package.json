{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio of Erik Gelderblom",
  "main": "app.js",
  "config": {
    "public_url": "/",
    "asset_url": "/static/"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --serve --quiet",
    "build:critical": "./node_modules/.bin/critical dist/index.html -c src/css/app.css --base / > src/css/critical.css",
    "build:eleventy": "npx @11ty/eleventy --config=.eleventy.js",
    "build:postcss": "mkdir ./src/css && postcss src/scss/**/*.css -d src/css",
    "build:snowpack": "snowpack build",
    "build:clean": "rm -rf ./dist/public/scss",
    "build": "NODE_ENV=production ELEVENTY_ENV=production run-s build:eleventy build:snowpack build:clean",
    "start": "snowpack dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikFontanel/portfolio-temp.git"
  },
  "author": "Erik Gelderblom",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikFontanel/portfolio-temp/issues"
  },
  "homepage": "/",
  "dependencies": {
    "load-bmfont": "^1.4.1",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@snowpack/plugin-babel": "^2.1.0",
    "@snowpack/plugin-parcel": "^1.3.0",
    "@swup/overlay-theme": "^1.1.1",
    "autoprefixer": "^9.8.6",
    "critical": "^2.0.3",
    "cssnano": "^4.1.10",
    "fitvids": "^2.1.1",
    "image-size": "^0.8.3",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-link-attributes": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "postcss-cli": "^7.1.1",
    "postcss-custom-media": "^7.0.8",
    "postcss-media-minmax": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.26.10",
    "snowpack": "^2.7.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 2 versions",
    "> 1%",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ]
}