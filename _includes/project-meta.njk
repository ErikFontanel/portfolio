<div class="project-meta text-xs">
  {% if single_thumbnail|length %}
    <h3 class="project-item--client  text-sub ">{{ client }}</h3>
  {% endif %}
  <dl>
    {% if role|length %}
      <dt>Role</dt>
      <dd>{{ role }}</dd>
    {% endif %}

    {% if team|length %}
      <dt>Team</dt>
      <dd>{{ team }}</dd>
    {% endif %}
  </dl>
</div>
